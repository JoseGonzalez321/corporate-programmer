
RewriteEngine on
RewriteCond %{HTTP_HOST} ^corporateprogrammer.com$
RewriteRule ^/?$ "http\:\/\/www\.corporateprogrammer\.com\/" [R=301,L]

SetEnvIfNoCase User-Agent "^libwww-perl*" block_bad_bots
Deny from env=block_bad_bots

# Use PHP5.6 as default
AddHandler application/x-httpd-php56 .php

ServerSignature Off

# 7 DAYS
<FilesMatch "\.(ico|pdf|flv|jpg|jpeg|png|gif|)$">
Header set Cache-Control "max-age=604800, public, must-revalidate, proxy-revalidate"
</FilesMatch>

# 7 DAYS
<FilesMatch "\.(js|css)$">
Header set Cache-Control "max-age=604800, public, must-revalidate, proxy-revalidate"
</FilesMatch>

# 2 DAYS
<FilesMatch "\.(xml|txt)$">
Header set Cache-Control "max-age=172800, public, must-revalidate, proxy-revalidate"
</FilesMatch>

# 2 HOURS
<FilesMatch "\.(html|htm)$">
Header set Cache-Control "max-age=172800, must-revalidate, proxy-revalidate"
</FilesMatch>
